---
exclude: !ruby/regexp '/
  \.(?:
      tmp |
      swp
    )
  $
  |
  \.(?:
       autotest |
       gemtest |
       gitignore |
       workenv |
       hoerc |
       minitest.rb |
       simplecov-prelude.rb
     )
  $
  |
  CVS\/
  |
  ^(?i:TAGS)
  |
  \.(?:
      DS_Store |
      bundle |
      git |
      hg |
      idea |
      svn |
      vagrant
    )\/
  |
  Appraisals
  |
  vendor\/
  |
  [gG]emfile(?:\.lock)?
  |
  \.(?:
      coveralls |
      pullreview |
      travis |
      rubocop.*
     )\.yml$
  |
  \.byebug_history
  |
  \.gemspec
  /x'
